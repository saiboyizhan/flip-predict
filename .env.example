# ============================================
# Frontend Environment Variables (Vite)
# ============================================
# Copy this file to .env and fill in the values.

# Backend API URL
VITE_API_URL=http://localhost:3001

# WebSocket URL
VITE_WS_URL=ws://localhost:3001

# WalletConnect Project ID
# Get yours at: https://cloud.walletconnect.com
VITE_WALLETCONNECT_PROJECT_ID=

# Contract addresses (set after deployment via `npm run deploy:bscTestnet`)
VITE_PREDICTION_MARKET_ADDRESS=
# Preferred key (keeps compatibility with existing code)
VITE_NFA_CONTRACT_ADDRESS=
# Backward-compatible alias (optional)
VITE_NFA_ADDRESS=
# USDT token address
VITE_USDT_ADDRESS=
